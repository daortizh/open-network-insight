                                "header3.1":"test"
                            }
                        },
                        {
                            "name": "column_set",
                            "config": {
                                "header3.1":"new_test"
                            }
                        },
                        {
                            "name": "column_rename",
                            "config": {
                                "header3.1": "header3"
                            }
                        },
                        {
                            "name": "debug"
                        }
                    ]
                },
                {
                    "filename": "threats.csv",
